import { Result } from "../types/fp.js";
/**
 * Get Discount
 *
 * @remarks
 * Get a discount by ID.
 *
 * **Scopes**: `discounts:read` `discounts:write`
 */
export declare function discountsGet(client: PolarCore, request: DiscountsGetRequest, options?: RequestOptions): APIPromise<Result<Discount, ResourceNotFound | HTTPValidationError | SDKError | SDKValidationError | UnexpectedClientError | InvalidRequestError | RequestAbortedError | RequestTimeoutError | ConnectionError>>;
